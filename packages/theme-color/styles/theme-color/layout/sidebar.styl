sidebar($themeColor)
  // 侧边栏
  .sidebar-heading.clickable
    &.active
      color $themeColor
      border-left-color $themeColor

    &:hover
      color $themeColor

  // 侧边栏链接
  a.sidebar-link
    &:hover
      color $themeColor

    &.active
      color $themeColor
      border-left-color $themeColor
